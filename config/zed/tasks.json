[
  {
    "label": "Lazygit",
    "command": "lazygit",
    "args": [],
    "env": {},
    "use_new_terminal": false,
    "allow_concurrent_runs": false
  },
  {
    "label": "mix format",
    "command": "mix",
    "args": ["format"],
    "env": {},
    "use_new_terminal": false,
    "allow_concurrent_runs": false
  },
  {
    "label": "mix test suite",
    "command": "mix",
    "args": ["test"]
  },
  {
    "label": "mix test file",
    "command": "mix",
    "args": ["test", "$ZED_FILE"]
  },
  {
    "label": "mix test line",
    "command": "mix",
    "args": ["test", "$ZED_FILE:$ZED_ROW"]
  },
  {
    "label": "mix test break line",
    "command": "iex",
    "args": ["-S", "mix", "test", "-b", "$ZED_FILE:$ZED_ROW"]
  },
  {
    "label": "mix test failed",
    "command": "mix ",
    "args": ["test", "--failed"]
  },
  {
    "label": "Beacon: mix dev core",
    "command": "iex",
    "args": ["--sname", "core", "-S", "mix", "dev"],
    "env": {},
    "use_new_terminal": false,
    "allow_concurrent_runs": false
  },
  {
    "label": "Beacon: mix dev admin",
    "command": "iex",
    "args": ["--sname", "admin", "-S", "mix", "dev"],
    "env": {},
    "use_new_terminal": false,
    "allow_concurrent_runs": false
  }
]
